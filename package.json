{
  "name": "@planktonzp/vsc-leetcode-cli",
  "version": "2.8.5",
  "description": "A cli tool to enjoy leetcode!",
  "engines": {
    "node": ">=20"
  },
  "bin": {
    "leetcode": "bin/leetcode"
  },
  "scripts": {
    "lint": "eslint lib/ test/",
    "test": "npm run lint && nyc mocha test test/plugins && nyc report --reporter=lcov",
    "travis": "node bin/pkg",
    "pkg": "pkg . --out-path=dist/ --targets"
  },
  "pkg": {
    "scripts": [
      "lib"
    ],
    "assets": [
      "colors",
      "icons",
      "templates"
    ],
    "targets": [
      "node20-linux-x64",
      "node20-macos-x64",
      "node20-win-x64"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/planktonzp/leetcode-cli.git"
  },
  "keywords": [
    "leetcode",
    "cli",
    "command",
    "tool"
  ],
  "author": "Eric Wang <skygragon@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leetcode-tools/leetcode-cli/issues"
  },
  "homepage": "https://github.com/leetcode-tools/leetcode-cli#readme",
  "dependencies": {
    "ansi-styles": "^3.2.1",
    "cheerio": "1.0.0-rc.12",
    "he": "1.2.0",
    "mkdirp": "3.0.1",
    "moment": "^2.29.1",
    "nconf": "^0.12.1",
    "ora": "^5.4.1",
    "prompt": "1.3.0",
    "request": "2.88.2",
    "supports-color": "^8.1.1",
    "underscore": "1.13.6",
    "wordwrap": "1.0.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "chai": "5.1.0",
    "eslint": "8.56.0",
    "eslint-config-google": "0.14.0",
    "mocha": "10.3.0",
    "nock": "13.5.3",
    "nyc": "^15.1.0",
    "pkg": "5.8.1",
    "rewire": "7.0.0"
  },
  "main": ".eslintrc.js",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  }
}
